<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>Math.random()</title>
</head>
<body>
<script>
  //补充代码
  var input=prompt("请输入你猜的数字");
  var num2=Math.floor(Math.random()*10+1);
  while(num2!=input){if(input>num2){
      alert("输入的数字大了");
      input=prompt("请输入你猜的数字");
  }else if(input<num2){
      alert("输入的数字小了");
      input=prompt("请输入你猜的数字");
  }
  }
      alert("恭喜你猜对了");
</script>
</body>
</html>
